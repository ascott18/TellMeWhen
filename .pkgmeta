package-as: TellMeWhen

externals:
    Lib/CallbackHandler-1.0: svn://svn.wowace.com/wow/callbackhandler/mainline/trunk/CallbackHandler-1.0
    Lib/LibStub: svn://svn.wowace.com/wow/libstub/mainline/trunk/

    Lib/libdatabroker-1-1: https://git.wowace.com/wow/libdatabroker-1-1/mainline.git

    Lib/LibSharedMedia-3.0: svn://svn.wowace.com/wow/libsharedmedia-3-0/mainline/trunk

    Lib/DRList-1.0: https://github.com/wardz/DRList-1.0

    Lib/LibDogTag-3.0: https://github.com/parnic/LibDogTag-3.0.git
    Lib/LibDogTag-Unit-3.0: https://github.com/parnic/LibDogTag-Unit-3.0.git
    Lib/LibDogTag-Stats-3.0: https://github.com/ascott18/LibDogTag-Stats-3.0.git

    Lib/LibSpellRange-1.0: https://github.com/ascott18/LibSpellRange-1.0.git
    Lib/LibOO-1.0: https://github.com/ascott18/LibOO-1.0.git
    
    Lib/LibRangeCheck-3.0: https://github.com/WeakAuras/LibRangeCheck-3.0

    Lib/AceAddon-3.0: svn://svn.wowace.com/wow/ace3/mainline/trunk/AceAddon-3.0
    Lib/AceEvent-3.0: svn://svn.wowace.com/wow/ace3/mainline/trunk/AceEvent-3.0
    Lib/AceComm-3.0: svn://svn.wowace.com/wow/ace3/mainline/trunk/AceComm-3.0
    Lib/AceConsole-3.0: svn://svn.wowace.com/wow/ace3/mainline/trunk/AceConsole-3.0
    Lib/AceDB-3.0: svn://svn.wowace.com/wow/ace3/mainline/trunk/AceDB-3.0
    Lib/AceLocale-3.0: svn://svn.wowace.com/wow/ace3/mainline/trunk/AceLocale-3.0
    Lib/AceTimer-3.0: svn://svn.wowace.com/wow/ace3/mainline/trunk/AceTimer-3.0
    Lib/AceSerializer-3.0: svn://svn.wowace.com/wow/ace3/mainline/trunk/AceSerializer-3.0

    Lib/LibBabble-CreatureType-3.0: svn://svn.wowace.com/wow/libbabble-creaturetype-3-0/mainline/trunk
    Lib/LibBabble-Race-3.0: svn://svn.wowace.com/wow/libbabble-race-3-0/mainline/trunk

optional-dependencies:
    - masque
    - omni-cc
    - tullacc
    - libdatabroker-1-1

manual-changelog:
    filename: CHANGELOG.short.md
    markup-type: markdown

license-output: LICENSE.txt
enable-nolib-creation: no

move-folders:
    TellMeWhen/TellMeWhen_Options: TellMeWhen_Options

ignore:
    - scrapper.lua
    - USEDLOCALE.lua
    - Options/scrapper.lua
    - Options/USEDLOCALE.lua
    - Scripts
    - build
    - Documentation
    - README.md
    - Localization/TellMeWhen-itIT.lua
    - Localization/TellMeWhen-frFR.lua
    - Localization/TellMeWhen-deDE.lua
    - Localization/TellMeWhen-koKR.lua
    - Localization/TellMeWhen-esMX.lua
    - Localization/TellMeWhen-ptBR.lua
